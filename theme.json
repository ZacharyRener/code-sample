				},
				"border": {
					"radius": "4px",
					"width": "1px",
					"color": "var(--wp--preset--color--yellow)"
				},
				"color": {
					"background": "var(--wp--preset--color--yellow)",
					"text": "var(--wp--preset--color--dark-gray)"
